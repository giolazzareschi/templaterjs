*
	padding: 0px
	margin: 0px
	border: 0px
	box-sizing: border-box
	z-index: 0

html,body
	height: 100%
	overflow: hidden

body
	font-family: 'Lato', sans-serif
	font-size: 13px
	color: #333
	position: relative

object
	display: none

ul,ol
	list-style: none
	
a
	color: blue
	
	&:visited
		color: blue
	&:active
		color: #05456b

input
	padding: 1em
	border: 1px solid #ccc